{
  "name": "wxent-api-redis",
  "version": "2.0.1",
  "description": "微信企业号API，使用Redis保存AccessToken",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo kriasoft/babel-starter-kit",
    "start": "easystatic start docs"
  },
  "keywords": [
    "wechat",
    "wechat-enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nagucc/wxent-api-redis.git"
  },
  "author": "",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/nagucc/wxent-api-redis/issues"
  },
  "homepage": "https://github.com/nagucc/wxent-api-redis",
  "jsnext:main": "index.es6.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  },
  "dependencies": {
    "access-token-redis": "^1.0.3",
    "redis": "^2.6.2",
    "wechat-enterprise-api": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "del": "^2.2.2",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^2.0.0-pre.3"
  }
}
